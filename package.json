{
  "name": "rocketact",
  "version": "0.1.8",
  "description": "Develop React projects with ease",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "commit": "git-cz",
    "compile": "rm -rf dist && tsc",
    "prepublishOnly": "npm run compile"
  },
  "keywords": [
    "rocketact",
    "react",
    "webpack",
    "zero-configuration",
    "build-tools",
    "TypeScript"
  ],
  "bin": {
    "rocketact": "./dist/bin/rocketact.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdf2e/rocketact.git"
  },
  "contributors": [
    {
      "name": "loveky",
      "email": "ylzcylx@gmail.com"
    },
    {
      "name": "beanlee",
      "email": "irobot039@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jdf2e/rocketact/issues"
  },
  "homepage": "https://github.com/jdf2e/rocketact",
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/node": "^10.12.2",
    "commitizen": "3.0.4",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.1.3",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.4",
    "prettier": "^1.14.3",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.1.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "commander": "^2.19.0",
    "fs-extra": "^7.0.1",
    "rocketact-dev-utils": "^1.0.0",
    "validate-npm-package-name": "^3.0.0"
  },
  "engines": {
    "node": ">=8"
  }
}
